<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="FlemStudio.AssetExplorerApplication.Avalonia.AssetContainerContentView"
			 xmlns:views="clr-namespace:FlemStudio.AssetExplorerApplication.Avalonia"
			 
			 >



	<ScrollViewer Background="{DynamicResource ThemeControlMidBrush}">

			<ItemsControl ItemsSource="{Binding Items}" Name="ItemsControl">
				<ItemsControl.ItemsPanel>
					<ItemsPanelTemplate>
						<WrapPanel Background="Transparent"/>

					</ItemsPanelTemplate>
				</ItemsControl.ItemsPanel>
				<ItemsControl.ItemTemplate>
					<DataTemplate>
						<views:AssetContainerContentItemView Content="{Binding}"></views:AssetContainerContentItemView>
					</DataTemplate>
				</ItemsControl.ItemTemplate>
			</ItemsControl>

	</ScrollViewer>

</UserControl>
